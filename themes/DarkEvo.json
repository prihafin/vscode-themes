{
  "$schema": "vscode://schemas/color-theme",
	"name": "Dark Evo",
	"type": "dark",
  "semanticHighlighting": true,

	"semanticTokenColors": {
		"class": {
      "fontStyle": "bold",
      "foreground":"#76f"
    },
		"interface": {
      "fontStyle": "italic",
      "foreground":"#76f"
    },

		"parameter": {
			"fontStyle": "",
			"foreground": "#b5c9ff"
		},

		"plainKeyword": {
			"fontStyle": "",
			"foreground": "#0081cc"
		},

    "variable.defaultLibrary": {
			"foreground": "#577f96"
    }
  },

	"tokenColors": [
    {
			"name": "base",
			"scope": [
        "source",
        "support.type.property-name",
        "punctuation.support.type.property-name",
        "meta.template.expression",
        "expression.ng",
        "punctuation.definition.list.begin.markdown"
      ],
			"settings": {
        "fontStyle": "",
				"foreground": "#e5e5e5"
			}
		},
		{
			"name": "comment",
			"scope": [
        "comment",
        "punctuation.definition.comment",
        "punctuation.definition.tag",
        "constant.character.escape.line-continuation.shell"
      ],
			"settings": {
				"fontStyle": "",
				"foreground": "#555555"
			}
		},
		{
			"name": "keyword",
			"scope": [
        "keyword.control.as",
        "keyword.control.try",
        "keyword.control.trycatch",
        "keyword.control.export",
        "keyword.control.flow",
        "keyword.control.import",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.from",
        "keyword.control.default",
        "keyword.control.type",
        "storage.modifier",
        "keyword.type",
        "keyword.other",
        "storage.type",
        "keyword.operator.new",
        "keyword.operator.expression",
        "support.function.builtin",
        "support.type.primitive",
        "support.type.builtin",
        "support.type.object.module.js",
        "variable.language.this",
        "variable.language.super",
        "keyword.operator.logical.python",
        "variable.language.special.self.python",
        "variable.parameter.function.language.special.self.python",
        "entity.name.tag.html",
        "keyword.control.powershell",
      ],
			"settings": {
        "foreground": "#0081cc"
			}
		},
		{
			"name": "builtins",
			"scope": [
        "constant.language",
        "support.variable.automatic.powershell",
      ],
			"settings": {
        "foreground": "#00629b",
        "fontStyle": ""
			}
		},
		{
			"name": "throw",
			"scope": [
        "keyword.control.throw"
      ],
			"settings": {
        "foreground": "#cc0000",
        "fontStyle": "bold italic"
			}
		},
		{
			"name": "return",
			"scope": [
        "disabled.keyword.control.flow.return"
      ],
			"settings": {
        "foreground": "#0081cc",
        "fontStyle": "bold italic"
			}
		},
		{
			"name": "true",
			"scope": [
        "constant.language.boolean.true",
        "constant.language.boolean.true.cs"
      ],
			"settings": {
				"foreground": "#00bf01"
			}
		},
		{
			"name": "false",
			"scope": [
        "constant.language.boolean.false",
        "constant.language.boolean.false.cs"
      ],
			"settings": {
				"foreground": "#ff7878"
			}
		},
		{
			"name": "method",
			"scope": [
        "entity.name.function",
        "meta.function-call.generic",
        "entity.other.ng-binding-name.event",
        "support.function.powershell"
      ],
			"settings": {
        "fontStyle": "",
				"foreground": "#ff8844"
			}
		},
		{
			"name": "parameter",
			"scope": [
        "constant.other.option",
      ],
			"settings": {
        "fontStyle": "",
				"foreground": "#0081cc"
			}
		},
		{
			"name": "jsdoc",
			"scope": [
        "storage.type.class.jsdoc",
      ],
			"settings": {
        "fontStyle": "bold",
				"foreground": "#ce6114"
			}
		},
		{
			"name": "doc",
			"scope": [
        "comment.documentation.cs"
      ],
			"settings": {
				"foreground": "#864"
			}
		},
		{
			"name": "doc",
			"scope": [
        "comment.documentation.name.cs"
      ],
			"settings": {
				"foreground": "#877e76"
			}
		},

    {
			"name": "flow control",
			"scope": [
      ],
			"settings": {
				"foreground": "#0d00ff"
			}
		},
		{
			"name": "operator",
			"scope": [
        "keyword.operator",
        "punctuation",
        "meta.brace",
        "storage.type.function.arrow",
        "entity.name.function.member.overload",
      ],
			"settings": {
				"foreground": "#aaaaaa"
			}
		},
		{
			"name": "string",
			"scope": [
        "string",
        "punctuation.definition.string"
      ],
			"settings": {
				"foreground": "#56a81b"
			}
		},
		{
			"name": "string",
			"scope": [
        "storage.type.string.python"
      ],
			"settings": {
				"foreground": "#316010"
			}
		},
		{
			"name": "number",
			"scope": [
        "constant.numeric",
        "keyword.other.unit",
        "entity.other.attribute-name.html"
      ],
			"settings": {
				"foreground": "#39c8ff"
			}
		},
		{
			"name": "class",
			"scope": [
        "entity.name.type.class",
        "entity.other.inherited-class",
        "entity.other.ng-binding-name.template"
      ],
			"settings": {
        "fontStyle": "bold",
				"foreground": "#9365a9"
			}
		},


		{
			"name": "heading",
			"scope": "markup.heading,punctuation.definition.heading",
			"settings": {
        "fontStyle": "bold",
				"foreground": "#7c99b4"
			}
		},
		{
			"name": "H1",
			"scope": "heading.1.markdown,heading.1.markdown punctuation.definition.heading",
			"settings": {
        "fontStyle": "bold underline",
				"foreground": "#ff0000"
			}
		},
		{
			"name": "H2",
			"scope": "heading.2.markdown,heading.2.markdown punctuation.definition.heading",
			"settings": {
        "fontStyle": "bold underline",
				"foreground": "#ce6114"
			}
		},
		{
			"name": "H3",
			"scope": "heading.3.markdown,heading.3.markdown punctuation.definition.heading",
			"settings": {
				"foreground": "#3b5cd1"
			}
		},
		{
			"name": "H4",
			"scope": "heading.4.markdown,heading.4.markdown punctuation.definition.heading",
			"settings": {
				"foreground": "#7c9dce"
			}
		},
		{
			"name": "list",
			"scope": "markup.list",
			"settings": {
				"foreground": "#cbcbcb"
			}
		},
		{
			"name": "quote",
			"scope": [
        "markup.quote",
        "markup.inline.raw.string"
      ],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "bold",
			"scope": "markup.bold",
			"settings": {
        "fontStyle": "bold"
			}
		},
		{
			"name": "italic",
			"scope": "markup.italic",
			"settings": {
        "fontStyle": "italic"
			}
		},



		{
			"name": "css",
			"scope": [
        "support.type.property-name.css",
      ],
			"settings": {
				"foreground": "#1ba89b"
			}
		},
		{
			"name": "css",
			"scope": [
        "entity.other.attribute-name.class.css",
        "entity.name.tag.css",
        "entity.other.attribute-name.pseudo-class.css",
      ],
			"settings": {
				"foreground": "#9365a9"
			}
		},
		{
			"name": "css",
			"scope": [
        "support.function.misc.scss",
      ],
			"settings": {
				"foreground": "#ff8844"
			}
		},

  ],

