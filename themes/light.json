{
  "$schema": "vscode://schemas/color-theme",
  "name": "Asseri Light",
  "type": "light",
  "colors": {
    "editorGutter.background": "#f6f6f6",
    "editorGutter.addedBackground": "#81b88b60",
    "editorGutter.deletedBackground": "#ca4b5160",
    "editorGutter.modifiedBackground": "#66afe060",
    "editor.wordHighlightBackground": "#ff800030",
    "editor.wordHighlightStrongBackground": "#ff800030",
    "editorBracketMatch.background": "#00000010",
    "editorBracketMatch.border": "#00000000",
    "editorLineNumber.foreground": "#00000040",
    "editorLineNumber.activeForeground": "#000000ff",
    "minimap.background": "#f8f8f8",
    "minimap.selectionHighlight": "#7070ff",
    "breadcrumb.background": "#ffffff",
    "breadcrumb.focusForeground": "#009000",
    "tab.border": "#00000018",
    "tab.activeBorderTop": "#00000000",
    "tab.activeBorder": "#00000000",

    "panel.background": "#f0f0f0",
    "panel.border": "#aaa",
    "panelTitle.activeForeground": "#000000",
    "panelTitle.activeBorder": "#000000",

    "terminal.background": "#182418",
    "terminal.foreground": "#AADDBB",
    "terminal.selectionBackground": "#aaaaaa",
    "scrollbar.shadow": "#bbbbbb",
    "titleBar.border": "#00000008"
  },
  "settings": {
    "settings": {
      "fontStyle": "",
      "foreground": "#000000d0",
      "background": "#f0f0f0",
      "selectionHighlight": "#a0a0ff"
    }
  },
  "tokenColors": [
    {
      "scope": "meta.object-literal.key.js",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "support.type.property-name.json",
      "settings": {
        "fontStyle": "",
        "foreground": "#505050"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin.json",
        "punctuation.definition.string.end.json"
      ],
      "settings": {
        "foreground": "#a0a0a0"
      }
    },
    {
      "scope": [
        "punctuation.definition.array.begin.json",
        "punctuation.definition.array.end.json",
        "punctuation.definition.dictionary.begin.json",
        "punctuation.definition.dictionary.end.json"
      ],
      "settings": {
        "foreground": "#404040"
      }
    },
    {
      "scope": [
        "string.quoted.double.json",
        "constant.numeric.json"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "invalid",
        "keyword.other.important.css"
      ],
      "settings": {
        "foreground": "#cd3131"
      }
    },
    {
      "scope": [
        "function.decorator",
        "meta.array.literal",
        "entity.name.function.decorator",
        "keyword.control.directive.include",
        "keyword.control.directive.define",
        "keyword.control.directive.pragma"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#007050"
      }
    },
    {
      "scope": [
        "meta.definition.function",
        "entity.name.function",
        "entity.name.tag",
        "variable.other.constant.object"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000c0"
      }
    },
    {
      "scope": [
        "!entity.name.type",
        "entity.name.class",
        "class.name",
        "variable.other.constant.object"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#700070e0"
      }
    },
    {
      "scope": [
        "token",
        "function.parameter.unpacking",
        "keyword",
        "keyword.operator.new",
        "keyword.operator.expression.delete",
        "keyword.operator.expression.of",
        "keyword.operator.expression.in",
        "keyword.operator.expression",
        "markup.bold",
        "constant.language",
        "support.type.builtin",
        "support.function.builtin",
        "support.constant.property-value",
        "entity.other.attribute-name.pseudo-class",
        "variable.language.super",
        "variable.language.this",
        "storage"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0000f0"
      }
    },
    {
      "scope": [
        "constant.language.python",
        "keyword.control.flow.python",
        "keyword.control.import.python",
        "keyword.operator.logical.python",
        "storage.type.function.lambda.python",
        "storage.type.function.python",
        "storage.type.class.python",
        "support.function.builtin.python"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e08000"
      }
    },
    {
      "scope": [
        "support.function",
        "support.magic.meta.function",
        "support.variable.object",
        "support.variable.dom",
        "support.constant",
        "support.type.object",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#904050"
      }
    },
    {
      "scope": [
        "disabled.variable.other.constant"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.numeric.hex",
        "constant.numeric",
        "property-value.numeric",
        "storage.type.number",
        "keyword.other.unit.css",
        "constant.other.color"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#0090e0"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "constant.regexp",
        "support.constant.font-name.css"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#009000"
      }
    },
    {
      "scope": [
        "string.quoted.double.js",
        "string.quoted.double.js punctuation.definition.string",
        "string.quoted.double.ts",
        "string.quoted.double.ts punctuation.definition.string"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#00d000"
      }
    },
    {
      "scope": [
        "constant.character.escape.js"
      ],
      "settings": {
        "foreground": "#005f0c60"
      }
    },
    {
      "scope": [
        "punctuation",
        "keyword.operator",
        "meta.brace"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#00000080"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#60300050"
      }
    },
    {
      "scope": [
        "cast.expr.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0000b0aa"
      }
    },
    {
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.other.html",
        "entity.name.tag.script.html",
        "entity.name.tag.style.html",
        "entity.name.tag.structure.any.html",
        "entity.name.tag.block.any.html"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#600060a0"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#b07000"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000080"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#800000"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#000080"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#09885a"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#a31515"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": [
        "markup.punctuation.quote",
        "markup.punctuation.list"
      ],
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": [
        "markup.quote",
        "markup.list"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#800000"
      }
    },
    {
      "scope": [
        "keyword.other.unit.px",
        "keyword.other.unit.percentage"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#0090e0"
      }
    },
    {
      "scope": [
        "variable.css"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#00000080"
      }
    }
  ]
}